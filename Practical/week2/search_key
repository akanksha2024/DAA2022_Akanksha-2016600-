#include<stdio.h>
#include<stdlib.h>
void search(int *arr,int n,int key)
{ int count=0;
for(int i=0;i<n;i++)
{
    if(arr[i]==key)
    {
        count++;
    }


}
if(count!=0)
printf("the key is found %d times",count);
else
    printf("\n key is not found in n array \n");


}
int main()
{
    int arr[100];
    int n,i;
 printf("Enter the size of an array");
    scanf("%d",&n);
     printf("Enter the elements of an array in sorted");
   for(i=0;i<n;i++)
    scanf("%d",&arr[i]);
    int key;
    printf("Enter the key to be found ");
    scanf("%d",&key);
    search(arr,n,key);
    return 0;

}
