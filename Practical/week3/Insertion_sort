#include<iostream>
using namespace std;
void sorting(int *arr,int n)
{int i,j,key,count=0,shifts=0;
    for(i=0;i<n;i++)
    { key=arr[i];
      j=i;
      if(arr[j]>arr[j+1])
    {
      shifts++; 
      
    }
    while(j>0&&arr[j-1]>key)
    {  
       arr[j]=arr[j-1];
        j=j-1;
        count++;
        
    }
      
      arr[j]=key;
      
    }
    
    cout<<"The Sorted Array is "<<endl;
    for(i=0;i<n;i++)
    {
        cout<<arr[i]<<" ";

    }
    cout<<endl;
    cout<<"The no. of compare"<<endl;
    cout<<count;
    cout<<endl;
    cout<<"The no. of shifts"<<endl;
    cout<<shifts;


}
int main()
{
    int arr[100],t,n,i;
    cout<<"Enter the test case"<<endl;
    cin>>t;
    while(t>0)
    { cin>>n;
      for(i=0;i<n;i++)
    {
        cin>>arr[i];

    } 
    sorting(arr,n);
    t--;
   }
    
   
    return 0;

}
